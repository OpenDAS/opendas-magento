<config>
    <modules>
        <Opendas_Shipping>
            <version>0.1.0</version>
        </Opendas_Shipping>
    </modules>
    <global>
        <models>
            <opendas_shipping>
                <class>Opendas_Shipping_Model</class>
            </opendas_shipping>
        </models>
        <resources>
            <opendas_shipping_setup>
                <setup>
                    <module>Opendas_Shipping</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </opendas_shipping_setup>
            <opendas_shipping_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </opendas_shipping_write>
            <opendas_shipping_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </opendas_shipping_read>
        </resources>
        <!--<blocks>
            <openerpconnector>
                <class>Openlabs_OpenERPConnector_Block</class>
            </openerpconnector>
        </blocks>-->
        <helpers>
            <opendas_shipping>
                <class>Opendas_Shipping_Helper</class>
            </opendas_shipping>
        </helpers>
    </global>
</config>